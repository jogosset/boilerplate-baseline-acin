{
  "definitions": [
    {
      "title": "Carousel",
      "id": "carousel",
      "model": "carousel",
      "filter": "carousel",
      "plugins": {
        "da": {
          "rows": 1,
          "columns": 2
        }
      }
    },
    {
      "title": "Carousel Slide",
      "id": "carousel-item",
      "model": "carousel-item",
      "plugins": {
        "da": {
          "rows": 1,
          "columns": 9,
          "fields": [
            { "name": "backgroundImage", "selector": "div:nth-child(1)>picture>img[src]" },
            { "name": "backgroundImageAlt", "selector": "div:nth-child(1)>picture>img[alt]" },
            { "name": "textBoxBackgroundColor", "selector": "div:nth-child(2)" },
            { "name": "textColor", "selector": "div:nth-child(3)" },
            { "name": "title", "selector": "div:nth-child(4)" },
            { "name": "titleColor", "selector": "div:nth-child(5)" },
            { "name": "titleBold", "selector": "div:nth-child(6)" },
            { "name": "body", "selector": "div:nth-child(7)" },
            { "name": "bodyColor", "selector": "div:nth-child(8)" },
            { "name": "bodyBold", "selector": "div:nth-child(9)" }
          ]
        }
      }
    }
  ],
